local Player = game:GetService("Players").LocalPlayer
local BlackListTable = {
	[1] = {
		["reason"] = "You have been banned from IceHub...BAD LIFE Need to unban ? Join discord on my ytb channel : IceHub"
	};
}

local BlackListCheck = BlackListTable[Player.UserId]

if BlackListCheck then
	Player:Kick("Blacklisted Reason: "..BlackListTable[Player.UserId]["reason"])
end
